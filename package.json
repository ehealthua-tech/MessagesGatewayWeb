    "postcss-math": "0.0.8",
    "postcss-nested": "1.0.0",
    "precss": "1.4.0",
    "prop-types": "15.6.0",
    "proxy-middleware": "0.15.0",
    "qs": "6.5.0",
    "react": "15.6.2",
    "react-dom": "15.6.2",
    "react-helmet": "4.0.0",
    "react-hot-loader": "4.3.3",
    "react-i18next": "2.2.0",
    "react-nebo15-validate": "0.1.12",
    "react-redux": "4.4.5",
    "react-router": "3.2.1",
    "react-router-redial": "0.3.4",
    "react-router-redux": "4.0.7",
    "react-sortable-hoc": "0.8.4",
    "redial": "0.5.0",
