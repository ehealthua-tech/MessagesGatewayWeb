{
  "name": "messages.gateway.admin.web",
  "version": "0.1.0",
  "description": "Messages Gateway Service",
  "license": "Apache License 2.0",
  "scripts": {
    "postinstall": "[ $PREBUILD ] && npm run build || exit 0",
    "predev": "npm run build",
    "dev": "concurrently --kill-others \"npm start\" \"node webpack.server.js\"",
    "preproduction": "NODE_ENV=production npm run build",
    "production": "NODE_ENV=production npm start",
    "prestart": "bin/runtime/inject-env-vars.js && bin/runtime/create-robots-txt.js",
    "start": "NODE_ENV=production node static/server.js",
    "build": "webpack",
    "test": "jest app --coverage",
    "test:debug": "jest app --no-cache",
    "e2e": "jest e2e --runInBand",
    "e2e:debug": "DEBUG=true jest e2e --runInBand --no-cache",
    "precommit": "lint-staged && npm test",
    "lint": "prettier \"**/*.{js,json,css,scss}\" --list-different",
    "locales:extract": "i18next-extract-gettext --files='./+(app)/**/*.+(js|json)' --output=app/common/locales/nhs.admin.web.pot"
  },
  "lint-staged": {
    "*.{js,json,css,scss}": [
      "prettier --write",
      "git add"
    ]
  },
  "browserslist": [
    "defaults",
    "> 1%",
    "iOS >= 7",
    "Safari >= 7",
    "Safari 8"
  ],
  "engines": {
    "node": "7.5.0"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.s?css$": "identity-obj-proxy"
    },
    "globals": {
      "__CLIENT__": true
    }
  },
  "dependencies": {
    "assets-webpack-plugin": "3.4.0",
    "autoprefixer": "6.7.7",
    "babel": "6.23.0",
    "babel-core": "6.26.3",
    "babel-loader": "6.4.1",
    "babel-plugin-module-resolver": "2.7.0",
    "babel-plugin-react-transform": "2.0.2",
    "babel-plugin-transform-decorators-legacy": "1.3.4",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-polyfill": "6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "6.23.0",
    "babel-preset-stage-0": "6.22.0",
    "babel-register": "6.26.0",
    "babel-runtime": "^6.26.0",
    "classnames": "2.2.5",
    "cookie-parser": "1.4.3",
    "core-js": "2.4.1",
    "cross-env": "2.0.0",
    "css-loader": "1.0.0",
    "cssnano": "^3.10.0",
    "date-fns": "1.27.2",
    "dotenv": "4.0.0",
    "ejs": "2.4.2",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-node": "^8.0.0",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "express": "^4.16.3",
    "extract-text-webpack-plugin": "2.1.0",
    "file-loader": "0.9.0",
    "history": "3.0.0",
    "http-status-codes": "1.1.6",
    "i18next": "7.0.1",
    "i18next-browser-languagedetector": "1.0.1",
    "i18next-express-middleware": "1.0.2",
    "i18next-po-loader": "1.0.0",
    "ignore-styles": "5.0.1",
